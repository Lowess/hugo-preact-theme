{
  "name": "exampleSite",
  "version": "1.0.0",
  "description": "Hugo Preact - Example Site",
  "repository": "Lowess/hugo-preact-theme",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./assets/js",
    "lint:fix": "eslint ./assets/js --fix",
    "dev": "hugo -d public --themesDir ../.. && run-p vite:**",
    "vite:start": "vite",
    "vite:hugo-watch": "onchange 'content/**/*.md' 'config.toml' '../layouts/**/*' -- npm run build:hugo",
    "start": "run-p start:**",
    "start:hugo": "hugo serve -d public -vw -p 3000",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "prebuild": "rm -rf public",
    "build": "npm run build:hugo && npm run build:vite",
    "build:hugo": "hugo -d public -v -b ${BASE_URL:-/} --themesDir ../..",
    "build:vite": "vite build",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@preact/preset-vite": "^2.6.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "ejs": "^3.1.9",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.1",
    "hugo-bin-extended": "^0.119.0",
    "inquirer": "^9.2.11",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "path": "^0.12.7",
    "toml": "^3.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@testing-library/preact": "^3.2.3",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "preact": "^10.18.1",
    "preact-habitat": "^3.3.0",
    "preact-island": "^1.1.2",
    "preact-render-to-string": "^6.2.2",
    "react": "npm:@preact/compat@^17.1.2",
    "react-dom": "npm:@preact/compat@^17.1.2",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.2.0",
    "sass": "^1.69.4"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}