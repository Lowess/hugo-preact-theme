{
  "name": "exampleSite",
  "version": "1.0.0",
  "description": "Hugo Preact - Example Site",
  "repository": "Lowess/hugo-preact-theme",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./assets/js",
    "lint:fix": "eslint ./assets/js --fix",
    "dev": "hugo -d public --themesDir ../.. && run-p vite:**",
    "vite:start": "vite",
    "vite:hugo-watch": "onchange 'content/**/*.md' 'config.toml' '../layouts/**/*' -- npm run build:hugo",
    "start": "run-p start:**",
    "start:hugo": "hugo serve -d public -vw -p 3000",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "prebuild": "rm -rf public",
    "build": "npm run build:hugo && npm run build:vite",
    "build:hugo": "hugo -d public -v -b ${BASE_URL:-/} --themesDir ../..",
    "build:vite": "vite build",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@preact/preset-vite": "^2.7.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "ejs": "^3.1.6",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fast-glob": "^3.2.11",
    "hugo-bin-extended": "^0.93.3",
    "inquirer": "^8.2.1",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "path": "^0.12.7",
    "sass": "^1.69.6",
    "toml": "^3.0.0",
    "typescript": "^4.6.2",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@testing-library/preact": "^2.0.1",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.1",
    "preact": "^10.6.6",
    "preact-habitat": "^3.3.0",
    "preact-island": "^1.1.2",
    "preact-render-to-string": "^5.1.20",
    "react": "npm:@preact/compat@^17.0.3",
    "react-dom": "npm:@preact/compat@^17.0.3",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.2.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  }
}
